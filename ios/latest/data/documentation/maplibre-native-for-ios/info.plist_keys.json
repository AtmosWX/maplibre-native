maplibre\/mlnofflinestorage\/databasepath","abstract":[{"type":"text","text":"The file path at which offline packs and the ambient cache are stored."}],"role":"symbol","title":"databasePath","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/databasePath","fragments":[{"kind":"identifier","text":"databasePath"}]},"https://developer.apple.com/documentation/uikit/text_display_and_fonts/adding_a_custom_font_to_your_app":{"title":"bundled with your application","identifier":"https:\/\/developer.apple.com\/documentation\/uikit\/text_display_and_fonts\/adding_a_custom_font_to_your_app","type":"link","titleInlineContent":[{"type":"text","text":"bundled with your application"}],"url":"https:\/\/developer.apple.com\/documentation\/uikit\/text_display_and_fonts\/adding_a_custom_font_to_your_app"},"customizing-fonts.html#server-side-fonts":{"type":"link","identifier":"customizing-fonts.html#server-side-fonts","title":"server-side rendering","titleInlineContent":[{"text":"server-side rendering","type":"text"}],"url":"customizing-fonts.html#server-side-fonts"}}}