":"topic","abstract":[{"code":"MLNShape","type":"codeVoice"},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"role":"symbol","kind":"symbol","title":"MLNShape","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","url":"\/documentation\/maplibre\/mlnshape","fragments":[{"kind":"identifier","text":"MLNShape"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addOverlay:":{"url":"\/documentation\/maplibre\/mlnmapview\/addoverlay:","navigatorTitle":[{"text":"addOverlay:","kind":"identifier"}],"kind":"symbol","type":"topic","fragments":[{"text":"- ","kind":"text"},{"text":"addOverlay:","kind":"identifier"}],"role":"symbol","title":"addOverlay:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addOverlay:","abstract":[{"type":"text","text":"Adds a single overlay object to the map."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"role":"symbol","fragments":[{"text":"MLNPolyline","kind":"identifier"}],"type":"topic","kind":"symbol","title":"MLNPolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"MLNPolyline"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"text":" ","type":"text"},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances, and the line segments that","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"url":"\/documentation\/maplibre\/mlnpolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"role":"symbol","title":"MLNPolylineFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","url":"\/documentation\/maplibre\/mlnpolylinefeature","type":"topic","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"MLNPolylineFeature"},{"type":"text","text":" object associates a polyline shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollectionFeature":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","type":"topic","role":"symbol","fragments":[{"text":"MLNShapeCollectionFeature","kind":"identifier"}],"title":"MLNShapeCollectionFeature","abstract":[{"text":"An ","type":"text"},{"code":"MLNShapeCollectionFeature","type":"codeVoice"},{"text":" object associates a shape collection with an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"url":"\/documentation\/maplibre\/mlnshapecollectionfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","type":"topic","role":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeCollection"}],"title":"MLNShapeCollection","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"MLNShapeCollection"},{"type":"text","text":" object represents a shape consisting of zero or more"},{"text":" ","type":"text"},{"type":"text","text":"distinct but related shapes that are instances of "},{"type":"codeVoice","code":"MLNShape"},{"type":"text","text":". The constituent"},{"type":"text","text":" "},{"type":"text","text":"shapes can be a mixture of different kinds of shapes."}],"url":"\/documentation\/maplibre\/mlnshapecollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","type":"topic","role":"symbol","fragments":[{"text":"MLNPointCollectionFeature","kind":"identifier"}],"title":"MLNPointCollectionFeature","abstract":[{"type":"text","text":"An "},{"code":"MLNPointCollectionFeature","type":"codeVoice"},{"type":"text","text":" object associates a point collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"url":"\/documentation\/maplibre\/mlnpointcollectionfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"kind":"symbol","role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}],"abstract":[{"text":"An ","type":"text"},{"code":"MLNPointAnnotation","type":"codeVoice"},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"text":" ","type":"text"},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"MLNPointAnnotation"},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"text":" ","type":"text"},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"url":"\/documentation\/maplibre\/mlnpointannotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"topic","role":"symbol","fragments":[{"kind":