ce"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource/initWithIdentifier:configurationURL:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource\/initWithIdentifier:configurationURL:","title":"initWithIdentifier:configurationURL:","role":"symbol","type":"topic","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"initWithIdentifier:configurationURL:"}],"abstract":[{"text":"Returns a raster tile source initialized with an identifier and configuration","type":"text"},{"type":"text","text":" "},{"text":"URL.","type":"text"}],"navigatorTitle":[{"kind":"identifier","text":"initWithIdentifier:configurationURL:"}],"url":"\/documentation\/maplibre\/mlnrastertilesource\/initwithidentifier:configurationurl:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource":{"type":"topic","title":"MLNRasterTileSource","kind":"symbol","url":"\/documentation\/maplibre\/mlnrastertilesource","abstract":[{"type":"codeVoice","code":"MLNRasterTileSource"},{"text":" is a map content source that supplies raster image tiles","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"text":" ","type":"text"},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"type":"text","text":" "},{"text":"the","type":"text"},{"type":"text","text":" "},{"text":"TileJSON specification","type":"text"},{"type":"text","text":"."},{"text":" ","type":"text"},{"type":"text","text":"A raster tile source is added to an "},{"type":"codeVoice","code":"MLNStyle"},{"text":" object along with one or more","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"MLNRasterStyleLayer"},{"text":" objects. Use a raster style layer to control the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"appearance of content supplied by the raster tile source."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","fragments":[{"text":"MLNRasterTileSource","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","type":"topic","title":"MapLibre","kind":"symbol","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]}}}