{"metadata":{"symbolKind":"method","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"setTargetCoordinate:animated:completionHandler:"}],"navigatorTitle":[{"text":"setTargetCoordinate:animated:completionHandler:","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:completionHandler:","title":"setTargetCoordinate:animated:completionHandler:","roleHeading":"Instance Method","role":"symbol","modules":[{"name":"MapLibre"}]},"abstract":[{"text":"Sets the geographic coordinate that is the subject of observation as the user","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"location is being tracked, with an optional transition animation and completion"},{"text":" ","type":"text"},{"type":"text","text":"handler."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"text":"setTargetCoordinate:","kind":"identifier"},{"kind":"text","text":"("},{"text":"CLLocationCoordinate2D","preciseIdentifier":"c:@S@CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"animated:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:@T@BOOL","text":"BOOL","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"animated"},{"text":" ","kind":"text"},{"text":"completionHandler:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"kind":"text","text":" (^"},{"text":")()) ","kind":"text"},{"kind":"internalParam","text":"completion"},{"text":";","kind":"text"}],"languages":["occ"]}]},{"parameters":[{"name":"targetCoordinate","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The target coordinate to fit within the viewport.