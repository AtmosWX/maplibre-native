apeCollection","abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"MLNShapeCollection"},{"text":" object represents a shape consisting of zero or more","type":"text"},{"type":"text","text":" "},{"type":"text","text":"distinct but related shapes that are instances of "},{"code":"MLNShape","type":"codeVoice"},{"text":". The constituent","type":"text"},{"type":"text","text":" "},{"text":"shapes can be a mixture of different kinds of shapes.","type":"text"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"title":"MLNPointCollectionFeature","fragments":[{"text":"MLNPointCollectionFeature","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnpointcollectionfeature","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","abstract":[{"type":"text","text":"An "},{"code":"MLNPointCollectionFeature","type":"codeVoice"},{"type":"text","text":" object associates a point collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"title":"MLNEmptyFeature","fragments":[{"kind":"identifier","text":"MLNEmptyFeature"}],"type":"topic","url":"\/documentation\/maplibre\/mlnemptyfeature","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"MLNEmptyFeature"},{"text":" object associates an empty shape with an optional","type":"text"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"title":"MLNPolylineFeature","fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"type":"topic","url":"\/documentation\/maplibre\/mlnpolylinefeature","role":"symbol","identifier":"doc:\/\/org.swif