{"abstract":[{"text":"Whether symbols in this layer avoid colliding with symbols in adjacent tiles.","type":"text"}],"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnsymbolstylelayer\/symbolavoidsedges"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"externalID":"c:objc(cs)MLNSymbolStyleLayer(py)symbolAvoidsEdges","role":"symbol","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"symbolAvoidsEdges","kind":"identifier"}],"roleHeading":"Instance Property","fragments":[{"kind":"identifier","text":"symbolAvoidsEdges"}],"symbolKind":"property","title":"symbolAvoidsEdges"},"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/symbolAvoidsEdges","interfaceLanguage":"occ"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer"]]},"primaryContentSections":[{"declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"text":"@property","kind":"keyword"},{"kind":"text","text":" ("},{"text":"nonatomic","kind":"keyword"},{"kind":"text","text":") "},{"preciseIdentifier":"c:objc(cs)NSExpression","text":"NSExpression","kind":"typeIdentifier"},{"kind":"text","text":" * "},{"text":"symbolAvoidsEdges","kind":"identifier"},{"kind":"text","text":";"}]}],"kind":"declarations"},{"content":[{"anchor":"discussion","text":"Discussion","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"text":"If this property is set to ","type":"text"},{"type":"codeVoice","code":"true"},{"text":", symbols in this layer avoid crossing the","type":"text"},{"type":"text","text":" "},{"text":"edge of a tile. You should set this property to ","type":"text"},{"type":"codeVoice","code":"true"},{"text":" if the backing vector","type":"text"},{"type":"text","text":" "},{"text":"tiles don’t have enough padding to prevent collisions, or if this layer’s","type":"text"},{"type":"text","text":" "},{"code":"symbolPlacement","type":"codeVoice"},{"type":"text","text":" property is set to "},{"type":"codeVoice","code":"MLNSymbolPlacementPoint"},{"type":"text","text":" but this layer"},{"type":"text","text":" "},{"type":"text","text":"is above a symbol layer whose "},{"type":"codeVoice","code":"symbolPlacement"},{"type":"text","text":" property is set to"},{"type":"text","text":" "},{"code":"MLNSymbolPlacementLine","type":"codeVoice"},{"type":"text","text":". You do not need to enable this property to prevent"},{"text":" ","type":"text"},{"type":"text","text":"clipped labels at tile boundaries."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The default value of this property is an expression that evaluates to "},{"type":"codeVoice","code":"NO"},{"text":". Set","type":"text"},{"type":"text","text":" "},{"type":"text","text":"this property to "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" to reset it to the default value."}]},{"inlineContent":[{"text":"This attribute corresponds to the ","type":"text"},{"text":"symbol-avoid-edges","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"layout property in the MapLibre Style Spec."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"You can set this property to an expression containing any of the following:","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"Constant Boolean values","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Predefined functions, including mathematical and string operators"},{"type":"text","text":" "},{"text":"Conditional expressions","type":"text"},{"text":" ","type":"text"},{"text":"Variable assignments and references to assigned variables","type":"text"},{"type":"text","text":" "},{"text":"Step functions applied to the ","type":"text"},{"type":"codeVoice","code":"$zoomLevel"},{"type":"text","text":" variable"}]},{"type":"paragraph","inlineContent":[{"text":"This property does not support applying interpolation functions to the","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"$zoomLevel"},{"type":"text","text":" variable or applying interpolation or step functions to feature"},{"type":"text","text":" "},{"text":"attributes.","type":"text"}]}],"kind":"content"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnsymbolstylelayer","title":"MLNSymbolStyleLayer","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"MLNSymbolStyleLayer"},{"text":" is a style layer that renders icon and text labels at","type":"text"},{"type":"text","text":" "},{"text":"points or along lines on the map.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNSymbolStyleLayer"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer/symbolAvoidsEdges":{"kind":"symbol","title":"symbolAvoidsEdges","abstract":[{"type":"text","text":"Whether symbols in this layer avoid colliding with symbols in adjacent tiles."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/symbolAvoidsEdges","type":"topic","fragments":[{"text":"symbolAvoidsEdges","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnsymbolstylelayer\/symbolavoidsedges"}}}