ultiPolygonFeature":{"role":"symbol","kind":"symbol","title":"MLNMultiPolygonFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","fragments":[{"text":"MLNMultiPolygonFeature","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmultipolygonfeature","abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"MLNMultiPolygonFeature"},{"type":"text","text":" object associates a multipolygon shape with an"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"url":"\/documentation\/maplibre\/mlnmultipolylinefeature","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","title":"MLNMultiPolylineFeature","role":"symbol","fragments":[{"text":"MLNMultiPolylineFeature","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"An ","type":"text"},{"code":"MLNMultiPolylineFeature","type":"codeVoice"},{"type":"text","text":" object associates a multipolyline shape with an"},{"type":"text","text":" "},{"text":"optional identifier and attributes.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"abstract":[{"type":"text","text":"An "},{"code":"MLNMultiPolygon","type":"codeVoice"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polygons that do not overlap. For example, you could use a multipolygon shape","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"type":"text","text":" "},{"text":"atoll: the inner island would be one ","type":"text"},{"type":"codeVoice","code":"MLNPolygon"},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"group of disconnected but related buildings."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","title":"MLNMultiPolygon","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/subtitle":{"kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"subtitle"}],"url":"\/documentation\/maplibre\/mlnshape\/subtitle","abstract":[{"text":"The subtitle of the shape annotation. The default value of this property is","type":"text"},{"type":"text","text":" "},{"code":"nil","type":"codeVoice"},{"type":"text","text":"."}],"type":"topic","title":"subtitle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/subtitle"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"role":"symbol","kind":"symbol","title":"MLNShape","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","fragments":[{"kind":"identifier","text":"MLNShape"}],"url":"\/documentation\/maplibre\/mlnshape","abstract":[{"type":"codeVoice","code":"MLNShape"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"url":"\/documentation\/maplibre\/mlnpointannotation","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}],"kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"code