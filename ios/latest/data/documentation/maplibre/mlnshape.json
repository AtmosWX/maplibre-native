 that forms the base map."}],"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","interfaceLanguage":"occ"},"relationshipsSections":[{"identifiers":["doc:\/\/org.swift.MyProject\/objc(cs)NSObject"],"kind":"relationships","title":"Inherits From","type":"inheritsFrom"},{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection"],"kind":"relationships","title":"Inherited By","type":"inheritedBy"},{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","doc:\/\/org.swift.MyProject\/objc(pl)NSSecureCoding"],"kind":"relationships","title":"Conforms To","type":"conformsTo"}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre"]]},"topicSections":[{"title":"Instance Properties","identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/subtitle","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/title"]},{"title":"Instance Methods","identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/geoJSONDataUsingEncoding:"]},{"title":"Type Methods","identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/shapeWithData:encoding:error:"]}],"variants":[{"paths":["\/documentation\/maplibre\/mlnshape"],"traits":[{"interfaceLanguage":"occ"}]}],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"@interface"},{"kind":"text","text":" "},{"kind":"identifier","text":"MLNShape"},{"kind":"tex