eters","parameters":[{"content":[{"inlineContent":[{"text":"The map view that requested the annotation view.","type":"text"}],"type":"paragraph"}],"name":"mapView"},{"content":[{"inlineContent":[{"type":"text","text":"The object representing the annotation that is about to be"},{"type":"text","text":" "},{"type":"text","text":"displayed."}],"type":"paragraph"}],"name":"annotation"}]},{"content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The view object to display for the given annotation or "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if you"},{"text":" ","type":"text"},{"type":"text","text":"want to display an annotation image instead."}]}],"kind":"content"},{"kind":"content","content":[{"type":"heading","anchor":"discussion","text":"Discussion","level":2},{"inlineContent":[{"type":"text","text":"Implement this method to mark a point annotation with a view object. If you"},{"text":" ","type":"text"},{"text":"want to mark a particular point annotation with a static image instead, omit","type":"text"},{"text":" ","type":"text"},{"text":"this method or have it return ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" for that annotation, then implement","type":"text"},{"type":"text","text":" "},{"code":"-mapView:imageForAnnotation:","type":"codeVoice"},{"type":"text","text":" instead."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Annotation views are compatible with UIKit, Core Animation, and other Cocoa"},{"type":"text","text":" "},{"type":"text","text":"Touch frameworks. On the other hand, static annotation images use less memory"},{"type":"text","text":" "},{"type":"text","text":"and draw more quickly than annotation views."}],"type":"paragraph"},{"inlineContent":[{"text":"The user location annotation view can also be customized via this method. When","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"annotation"},{"type":"text","text":" is an instance of "},{"code":"MLNUserLocation","type":"codeVoice"},{"text":" (or equal to the map view’s","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"userLocation"},{"text":" property), return an instance of ","type":"text"},{"type":"codeVoice","code":"MLNUserLocationAnnotationView"},{"type":"text","text":" "},{"text":"(or a subclass thereof).","type":"text"}],"type":"paragraph"},{"anchor":"Related-examples","type":"heading","text":"Related examples","level":4},{"type":"paragraph","inlineContent":[{"text":"TODO: Add annotation views and images, learn how to specify what","type":"text"},{"text":" ","type":"text"},{"code":"MLNViewAnnotation","type":"codeVoice"},{"text":" to use for a given ","type":"text"},{"code":"MLNPointAnnotation","type":"codeVoice"},{"text":" object on your","type":"text"},{"type":"text","text":" "},{"type":"text","text":"map."}]}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"abstract":[{"type":"text","text":"The "},{"type":"codeVoice","code":"MLNMapViewDelegate"},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"type":"codeVoice","code":"MLNMapView"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"text":"individual annotations.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","url":"\/documentation\/maplibre\/mlnmapviewdelegate","title":"MLNMapViewDelegate","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"fragments":[{"kind":"text","text":"- "},{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","navigatorTitle":[{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","title":"mapView:viewForAnnotation:","kind":"symbol","abstract":[{"type":"text","text":"Returns a view object to mark the given point annotation object on the map."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","role":"collection","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"role":"symbol","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"The ","type":"text"},{"code":"MLNAnnotation","type":"codeVoice"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotation","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","title":"MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"abstract":[{"type":"text","text":"The "},{"code":"MLNAnnotationView","type":"codeVoice"},{"text":" class is responsible for marking a point annotation","type":"text"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"text":" ","type":"text"},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"type":"text","text":" "},{"text":"offscreen.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","title":"MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","role":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"type":"text","text":"provided by Apple’s MapKit."}],"fragments":[{"text":"MLNMapView","kind":"identifier"}]}}}