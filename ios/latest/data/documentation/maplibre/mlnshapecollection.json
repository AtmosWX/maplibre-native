":"symbol","abstract":[{"text":"Creates and returns a shape collection consisting of the given shapes.","type":"text"}],"url":"\/documentation\/maplibre\/mlnshapecollection\/shapecollectionwithshapes:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection\/shapeCollectionWithShapes:","type":"topic","navigatorTitle":[{"text":"shapeCollectionWithShapes:","kind":"identifier"}],"kind":"symbol","fragments":[{"kind":"text","text":"+ "},{"kind":"identifier","text":"shapeCollectionWithShapes:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"kind":"symbol","type":"topic","title":"MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"MLNPointCollection"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"disconnected vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"text":"orchard.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"url":"\/documentation\/maplibre\/mlnpolyline","abstract":[{"text":"An ","type":"text"},{"code":"MLNPolyline","type":"codeVoice"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances, and the line segments that",