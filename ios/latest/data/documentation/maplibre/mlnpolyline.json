ments":[{"kind":"identifier","text":"MLNShapeCollection"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","title":"MLNShapeCollection","abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"MLNShapeCollection"},{"type":"text","text":" object represents a shape consisting of zero or more"},{"type":"text","text":" "},{"text":"distinct but related shapes that are instances of ","type":"text"},{"code":"MLNShape","type":"codeVoice"},{"text":". The constituent","type":"text"},{"type":"text","text":" "},{"type":"text","text":"shapes can be a mixture of different kinds of shapes."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnshapecollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygonFeature":{"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"MLNMultiPolygonFeature"},{"text":" object associates a multipolygon shape with an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"optional identifier and attributes."}],"url":"\/documentation\/maplibre\/mlnmultipolygonfeature","kind":"symbol","title":"MLNMultiPolygonFeature","fragments":[{"text":"MLNMultiPolygonFeature","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"MLNMultiPolygon"},{"type":"text","text":" object represents a shape consisting of one or more"},{"text":" ","type":"text"},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"text":" ","type":"text"},{"type":"text"