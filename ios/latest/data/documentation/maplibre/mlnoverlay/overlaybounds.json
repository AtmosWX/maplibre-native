re\/MLNOverlay"]]},"schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"title":"overlayBounds","modules":[{"name":"MapLibre"}],"role":"symbol","fragments":[{"kind":"identifier","text":"overlayBounds"}],"roleHeading":"Instance Property","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","symbolKind":"property","navigatorTitle":[{"kind":"identifier","text":"overlayBounds"}]},"variants":[{"paths":["\/documentation\/maplibre\/mlnoverlay\/overlaybounds"],"traits":[{"interfaceLanguage":"occ"}]}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"title":"overlayBounds","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"overlayBounds"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","fragments":[{"kind":"identifier","text":"MLNCoordinateBounds"}],"kind":"symbol","type":"topic","title":"MLNCoordinateBounds","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"url":"\/documentation\/maplibre\/mlncoordinatebounds","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"fragments":[{"text":"MLNOverlay","kind":"identifier"}],"type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","title":"MLNOverlay","kind":"symbol","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"text":" protocol defines a specific type of a